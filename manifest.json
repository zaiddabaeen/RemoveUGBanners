{
  "name": "Remove UG Ads",
  "description": "Removes UG Ads",
    "icons": {"16" : "download.png","48":"download.png", "128":"download.png"},
  "version": "1.0",
  "permissions": [
    "tabs", "*://*.ultimate-guitar.com/*"
  ],
   "content_scripts":[
                        {
                        "matches": ["*://*.ultimate-guitar.com/*"],
                        "js": ["popup.js"],
                        "run_at": "document_end"
                        }]
   ,
  "manifest_version": 2
}
